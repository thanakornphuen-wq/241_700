    งานอดิเรก:
    <input type="checkbox" name="fname" value="เล่นกีฬา"> ฟุตบอล
    <input type="checkbox" name="fname" value="ฟังเพลง"> บาสเกตบอล
    <input type="checkbox" name="fname" value="อ่านหนังสือ"> ว่ายน้ำ
    <script>
          let checkboxes=document.querySelectorAll("input[type='checkbox']" );
            console.log(checkboxes);
        for (let i=0;i<checkboxes.length;i++){
            console.log(checkboxes[i].value);
        }
    </script>

    <body>
    <button onclick="userClick()">Click Me</button>
    <div onclick="divclick()">Click div</div>
    
    <script>
        function userClick() {
            console.log("Button was clicked!");
        
        }
        function divclick() {
            console.log("Div was clicked!");
        }
    </script> 